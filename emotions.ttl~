@prefix em: <http://purl.org/vocab/emotion/> .

em:emotionOntology rdf:type skos:ConceptScheme;
	dct:title "Bible Emotion Ontology";
	dct:creator "INVENiT";
	skos:hasTopConcept em:emotion;
	skos:hasTopConcept em:appraisal.

em:emotion rdf:type skos:Concept;
	skos:prefLabel "emotion"@en,
	"emoties"@nl;
	skos:altLabel "emotion"@en;
	skos:altLabel "emotie"@nl;
	skos:inScheme em:emotionOntology.

